{
    "env": {
        "browser": true,
        "es2020": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    }
}